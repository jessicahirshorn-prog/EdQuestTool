{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/status",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/generate",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/preview",
      "destination": "/api/index.py"
    }
  ]
}
